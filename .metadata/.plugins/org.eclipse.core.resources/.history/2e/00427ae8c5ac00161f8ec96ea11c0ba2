package com.mulesoft.custom.kcs;

import java.util.HashMap;

public class DiffMap {
	
	public HashMap<String, String> showDiff(HashMap<String, String> map1, HashMap<String, String> map2) {
		
		HashMap<String, String> items = new HashMap <String, String>();
		
	    for (String ch1 : map1.keySet()) {
	    	
	    	String val1 = map1.get(ch1);
	        String val2 = map2.get(ch1);
	        
	        System.out.println("**** Value1 = " + val1);
	        System.out.println("**** Value2 = " + val1);
	        
	        
	        if (!val1.equalsIgnoreCase(val2))
	        	items.put(ch1, val2);
	    }
		return items;
	}
}
