package com.mule.custom.kcs;

import java.util.HashMap;
import java.util.LinkedHashMap;

public class TransformIdValue {
	
	private HashMap map = new HashMap();
	private String outStr;
	private String value;
	
	public HashMap doTransform(String key, HashMap<String, String> hashMap) {
		
		value = hashMap.get(key);
		byte[] decodedValue = java.util.Base64.getDecoder().decode(value);
		value = new String (decodedValue);
		
		switch (key) {
			case "11": outStr = "Eleven";
				break;
			case "12": outStr = "Twelve";
				break;
			case "13": outStr = "Thirteen";
				break;
			case "14": outStr = "Fourteen";
				break;
			default: outStr = "empty";
			}
		
		map.put(outStr, value);
		return map;
	}

}
